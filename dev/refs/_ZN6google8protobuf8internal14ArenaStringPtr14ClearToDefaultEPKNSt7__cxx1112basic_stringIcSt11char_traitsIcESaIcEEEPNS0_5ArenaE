<def f='include/google/protobuf/arenastring.h' l='231' ll='241' type='void google::protobuf::internal::ArenaStringPtr::ClearToDefault(const ::std::string * default_value, ::google::protobuf::Arena * )'/>
<doc f='include/google/protobuf/arenastring.h' l='228'>// Clears content, but keeps allocated string if arena != NULL, to avoid the
  // overhead of heap operations. After this returns, the content (as seen by
  // the user) will always be equal to |default_value|.</doc>
